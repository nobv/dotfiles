{
    "..": "cd ..",
    "..2": "cd ../..",
    "..3": "cd ../../..",
    "..4": "cd ../../../..",
    "..5": "cd ../../../../..",
    "cat": "bat",
    "chrome": "open -a Google Chrome",
    "cp": "cp -i",
    "dcd": "docker compose down",
    "dcu": "docker compose up -d",
    "gg": "REPO=$(ghq list | sort -u | peco);for GHQ_ROOT in $(ghq root -all);do [ -d $GHQ_ROOT/$REPO] && cd $GHQ_ROOT/$REPO;done",
    "gh": "hub browse $(ghq list | grep github.com | peco | cut -d '/' -f 2,3)",
    "home": "cd ~",
    "k": "kubectl",
    "l": "exa --git --icons -a",
    "ll": "exa --long --header --git --icons -a",
    "ls": "exa",
    "nix-repl": "nix repl '<nixpkgs>'",
    "mv": "mv -i",
    "ps": "procs",
    "reload": "exec $SHELL -l",
    "rm": "rm -i",
    "sed": "gsed",
    "tfa": "terraform apply",
    "tfi": "teffaform init",
    "tfp": "terraform plan",
    "tg": "terragrunt"
}